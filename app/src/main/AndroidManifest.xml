<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="maneewan.fms_job">

    <uses-sdk android:minSdkVersion="8" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashScreen"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GridViewActivity" />
        <activity
            android:name=".FeedNewsActivity"
            android:configChanges="orientation|screenSize"
            android:label="ข่าวสาร" />
        <activity
            android:name=".CompanyActivity"
            android:configChanges="orientation|screenSize"
            android:label="ค้นหาข้อมูลหน่วยงาน" />
        <activity
            android:name=".JobActivity"
            android:configChanges="orientation|screenSize"
            android:label="ค้นหาตำแหน่งงาน" />
        <activity
            android:name=".LayoutActivity"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="landscape"
            android:label="แผนผังงาน" />
        <activity
            android:name=".PollActivity"
            android:configChanges="orientation|screenSize"
            android:label="ประเมินความพึงพอใจ" />
        <activity android:name=".ExitActivity" />
        <activity android:name=".Company_groupActivity" />
        <activity android:name=".Company_allActivity" />
        <activity android:name=".Job_majorActivity" />
        <activity android:name=".Job_companyActivity" />
        <activity android:name=".Job_allActivity" />
        <activity android:name=".Job_position_by_major" />
        <activity android:name=".Job_position_by_company" />
        <activity android:name=".Company_by_group" />
        <activity android:name=".News_detail" />
        <activity android:name=".GridLayout"></activity>
    </application>

</manifest>
